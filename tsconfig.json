{
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["docs/components/*"]
    }
  },
  "include": ["env.d.ts", "docs/**/*.ts", "docs/components/**/*.vue"],
  "exclude": ["node_modules"]
}
